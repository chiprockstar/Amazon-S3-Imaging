<p id="notice"><%= notice %></p>

<h1>Thumbnail Images</h1>


  <tbody>
    <%= will_paginate @users%>
    <% @users.each do |user| %>
      <!-- <tr> -->
        <!-- <td> -->

      <div style="float:left;margin:10px;width:200px;height:300px;padding:10px;"></br>
        <%= link_to image_tag(user.avatar_url, width:200), user_path(user, page: params[:page]) %></br>
        <%= user.name %></br>
        <%= link_to 'Show', user_path(user, page: params[:page]) %>
        <%= link_to 'Edit', edit_user_path(user, page: params[:page]) %>
        <%= link_to 'Destroy', user_path(user, page: params[:page]), method: :delete, data: { confirm: 'Are you sure?' } %>
      </div>

      <!-- </tr> -->
    <% end %>
  </tbody>


<br>
<div style="clear:left;margin:10px;">
<%= link_to 'New Image', new_user_path %>
</div>
